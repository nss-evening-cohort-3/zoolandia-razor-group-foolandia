@{
    ViewBag.Title = "All Employees";
}

<div class="jumbotron">
    <h1>Foolandia</h1>
    <p class="lead">Foolandia, A Very Special Place To Visit!</p>
</div>
<div class="row">
    <div class="col-md-6">
        <h3>All Employees</h3>
       
        <ul class="list-group">
            @{foreach (var Employee in ViewBag.Employees)
                {
                    <li class="list-group-item">Name: <a href="/Employee/Details/@Employee.EmployeeId">@Employee.Name</a></li>
                }
            }
        </ul>
    </div>
</div>
